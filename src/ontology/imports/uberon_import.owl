        <obo:IAO_0000412 rdf:resource="&obo;uberon.owl"/>
        <rdfs:label rdf:datatype="&xsd;string">spleen</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/UBERON_0002107 -->

    <owl:Class rdf:about="&obo;UBERON_0002107">
        <obo:IAO_0000115 rdf:datatype="&xsd;string">An exocrine gland which secretes bile and functions in metabolism of protein and carbohydrate and fat, synthesizes substances involved in the clotting of the blood, synthesizes vitamin A, detoxifies poisonous substances, stores glycogen, and breaks down worn-out erythrocytes[GO].</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="&obo;uberon.owl"/>
        <rdfs:label rdf:datatype="&xsd;string">liver</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/UBERON_0002113 -->

