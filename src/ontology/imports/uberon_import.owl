        <obo:IAO_0000412 rdf:resource="&obo;uberon.owl"/>
        <rdfs:label rdf:datatype="&xsd;string">thymus</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/UBERON_0002371 -->

    <owl:Class rdf:about="&obo;UBERON_0002371">
