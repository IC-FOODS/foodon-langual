        <obo:IAO_0000412 rdf:resource="&obo;uberon.owl"/>
        <rdfs:label rdf:datatype="&xsd;string">leg</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/UBERON_0001264 -->

