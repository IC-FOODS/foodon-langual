	<rdfs:subClassOf rdf:resource="&obo;FOODON_03400853"/>
	<obo:IAO_0000412>http://langual.org</obo:IAO_0000412>
	<obo:IAO_0000115 xml:lang="en">includes yeast, baking powder, pectin, additives.</obo:IAO_0000115>
	<obo:IAO_0000114 rdf:resource="&obo;IAO_0000428"/>
	<oboInOwl:hasDbXref>LANGUAL:A0854</oboInOwl:hasDbXref>
</owl:Class>

<owl:Class rdf:about="&obo;FOODON_03400855">
	<rdfs:label xml:lang="en">flavouring or essence (eurofir)</rdfs:label>
