	<obo:IAO_0000412>http://langual.org</obo:IAO_0000412>
	<obo:IAO_0000115 xml:lang="en">e.g. tomato purée, sun-dried tomatoes, pickled red cabbage, sauerkraut.</obo:IAO_0000115>
	<obo:IAO_0000114 rdf:resource="&obo;IAO_0000428"/>
	<oboInOwl:hasDbXref>LANGUAL:A0827</oboInOwl:hasDbXref>
</owl:Class>

