	<oboInOwl:hasDbXref>LANGUAL:A0851</oboInOwl:hasDbXref>
</owl:Class>

<owl:Class rdf:about="&obo;FOODON_03400852">
	<rdfs:label xml:lang="en">miscellaneous food product (eurofir)</rdfs:label>
	<rdfs:subClassOf rdf:resource="&obo;FOODON_03400777"/>
	<obo:IAO_0000412>http://langual.org</obo:IAO_0000412>
	<obo:IAO_0000115 xml:lang="en">EFG group 32, Eurocode-2 group 12.</obo:IAO_0000115>
	<obo:IAO_0000114 rdf:resource="&obo;IAO_0000428"/>
	<rdfs:comment xml:lang="en">LanguaL curation note: Use for foods and ingredients that could not fit into any of the above classes.</rdfs:comment>
