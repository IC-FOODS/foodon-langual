	<obo:IAO_0000412>http://langual.org</obo:IAO_0000412>
	<obo:IAO_0000114 rdf:resource="&obo;IAO_0000428"/>
	<oboInOwl:hasDbXref>LANGUAL:A0657</oboInOwl:hasDbXref>
</owl:Class>

<owl:Class rdf:about="&obo;FOODON_03400658">
	<rdfs:label xml:lang="en">10  invertebrate animals (ccpr)</rdfs:label>
	<rdfs:subClassOf rdf:resource="&obo;FOODON_03400645"/>
	<obo:IAO_0000412>http://langual.org</obo:IAO_0000412>
	<obo:IAO_0000114 rdf:resource="&obo;IAO_0000428"/>
